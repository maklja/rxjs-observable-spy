{
	"name": "@maklja90/chaijs-rxjs-observable-spy",
	"version": "0.2.0",
	"description": "ChaiJS extension for testing RxJS observables",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf dist/",
		"build": "npm run clean && tsc",
		"test": "mocha --trace-warnings --recursive ./dist/**/*.test.js --sort",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts --config .prettierrc"
	},
	"keywords": [
		"chaijs",
		"chaijs extension",
		"rxjs",
		"observable",
		"mocks",
		"spies",
		"TypeScript Tests"
	],
	"author": "Bojan Makljenovic",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/maklja/rxjs-observable-spy"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"dependencies": {
		"@maklja90/rxjs-observable-spy": "^0.2.0",
		"@types/chai": "^4.3.0",
		"@types/deep-eql": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^18.7.16"
	},
	"peerDependencies": {
		"chai": ">=4.0.0",
		"deep-eql": ">=3.0.0",
		"rxjs": ">=6.0.0",
		"typescript": ">=4.0.0"
	}
}
