{
	"name": "@maklja90/rxjs-observable-spy",
	"version": "0.0.4",
	"description": "A simple little library that helps testing RxJS observables",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf dist/",
		"build": "npm run clean && tsc",
		"test": "mocha --trace-warnings --recursive ./dist/**/*.test.js --sort",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts --config .prettierrc"
	},
	"keywords": [
		"rxjs",
		"observable",
		"mocks",
		"spies",
		"TypeScript Tests"
	],
	"author": "Bojan Makljenovic",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/maklja/rxjs-observable-spy/tree/master/packages/observableSpy"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"chai": "^4.3.4"
	},
	"peerDependencies": {
		"rxjs": ">=6.0.0",
		"typescript": ">=4.0.0"
	}
}
