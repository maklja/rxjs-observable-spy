{
	"name": "@maklja/rxjs-observable-spy",
	"version": "0.0.1",
	"description": "A simple little class that helps testing Observables",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf dist/",
		"build": "npm run clean && tsc",
		"build-prod": "npm run clean && tsc --project tsconfig.prod.json",
		"test": "mocha --trace-warnings --recursive ./dist/test/**/*.test.js --sort",
		"lint": "eslint src/**/*.ts test/**/*.ts",
		"format": "prettier --write src/**/*.ts test/**/*.ts --config .prettierrc"
	},
	"keywords": [
		"rxjs",
		"observable",
		"mocks",
		"spies",
		"TypeScript Tests"
	],
	"author": "Bojan Makljenovic",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/maklja/rxjs-observable-spy"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.com"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"@types/mocha": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"chai": "^4.3.4",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-chai-expect": "^3.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"mocha": "^9.0.3",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2"
	},
	"peerDependencies": {
		"rxjs": ">=6.0.0",
		"typescript": ">=4.0.0"
	}
}
